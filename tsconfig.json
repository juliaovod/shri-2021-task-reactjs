{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "outDir": "./dist/",
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "UiKit/*": ["src/ui/*"]
    },
    "jsx": "react"
  },
  "exclude": ["node_modules"]
}
